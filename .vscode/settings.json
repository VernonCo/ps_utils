{
    "python.pythonPath": "venv/bin/python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc":{"when":"${basename}.py"},
        "**/__pycache__": true
    },
    "python.formatting.provider": "yapf"
}
